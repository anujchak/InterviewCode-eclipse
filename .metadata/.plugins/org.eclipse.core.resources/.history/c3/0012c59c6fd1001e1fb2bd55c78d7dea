package array;

public class test11 {

	public static void main(String[] args) {
		//arr = [1,2,4,3,2]
		//1-->2-->4-->2-->4  arr[4]=2 arr[arr[4]]=4
		
		int arr[]= {1,2,4,3,2};
		System.out.println(fun(arr));

	}
	
	public static int fun(int arr1[])
	{
		int i=0;
		int k=0;
		while(k != arr1[arr1[k]])
		{   System.out.println(arr1[k]);
		    System.out.println("k"+k);
			k=arr1[k];
			i++;
		}
		return i;
	}
	
	

}
