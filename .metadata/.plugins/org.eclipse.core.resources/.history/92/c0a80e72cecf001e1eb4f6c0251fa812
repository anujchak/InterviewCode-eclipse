package array;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.Map;
import java.util.function.Function;
import java.util.stream.Collectors;

public class test6 {

	private  Employee var;
	private  Employee var2;
	private  Employee var3;

	public static void main(String[] args) {
		
		 var = new Employee();
		 
		 var2= new Employee(2,"Anuju");
		 
		 var3= new Employee(3,"Anujuu");
		 
		 List<Employee> l = new ArrayList<Employee>();
		 l.add(var);
		 l.add(var2);
		 l.add(var3);
		 
		 l.stream().forEach(e->e.setDept("sales"));
		 
		for(Employee k:l) {
		 
           System.out.println(k.getId()+" "+k.getName()+" "+k.getDept());
		}
		
	}
}
