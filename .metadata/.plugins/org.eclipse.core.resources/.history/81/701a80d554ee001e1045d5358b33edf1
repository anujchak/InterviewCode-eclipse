public class test6{

	
	public static void main(String[] args) {
	
	List<Integer>l= new ArrayList<Integer>();
	l.add(1);
	l.add(2);
	l.add(3);
	l.add(4);
	System.out.println(l.stream().mapToInt(e->e).sum());
	String s = "hello";
	String s1="";
	List<String>l1=Arrays.asList(s.split(""));
